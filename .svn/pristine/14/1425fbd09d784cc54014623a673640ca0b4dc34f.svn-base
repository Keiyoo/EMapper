<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/util_bg"
    android:orientation="vertical"
    tools:context=".UnitSetActivity" >

    <include layout="@layout/layout_title_bar" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_margin="10dip"
        android:background="@drawable/layout_bg" >

        <TextView
            android:id="@+id/text_speed"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:drawableLeft="@drawable/speed" 
            style="@style/common_style_for_20sp_white"
            android:text="@string/speed"/>
        <TextView
            android:id="@+id/text_speed_content"
            android:layout_toRightOf="@+id/text_speed"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:layout_marginLeft="20dip"
            style="@style/common_style_for_24sp"
            android:text="@string/speed_set1"/>
         <TextView
              android:id="@+id/text_speed_set"
             android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:background="@drawable/more_bg"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_margin="10dip"
        android:background="@drawable/layout_bg" >

        <TextView
            android:id="@+id/text_length"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:drawableLeft="@drawable/length" 
            style="@style/common_style_for_20sp_white"
            android:text="@string/length"/>
        <TextView
            android:id="@+id/text_length_content"
            android:layout_toRightOf="@+id/text_length"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:layout_marginLeft="20dip"
            style="@style/common_style_for_24sp"
            android:text="@string/length_set1"/>
         <TextView
              android:id="@+id/text_length_set"
             android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:background="@drawable/more_bg"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_margin="10dip"
        android:background="@drawable/layout_bg" >

        <TextView
            android:id="@+id/text_area"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:drawableLeft="@drawable/mianji" 
            style="@style/common_style_for_20sp_white"
            android:text="@string/area"/>
        <TextView
            android:id="@+id/text_area_content"
            android:layout_toRightOf="@+id/text_area"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:layout_marginLeft="20dip"
            style="@style/common_style_for_24sp"
            android:text="@string/area_set1"/>
         <TextView
              android:id="@+id/text_area_set"
             android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:background="@drawable/more_bg"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_margin="10dip"
        android:background="@drawable/layout_bg" >

        <TextView
            android:id="@+id/text_time"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:drawableLeft="@drawable/time" 
            style="@style/common_style_for_20sp_white"
            android:text="@string/time_set"/>
        <TextView
            android:id="@+id/text_time_content"
            android:layout_toRightOf="@+id/text_time"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:layout_marginLeft="20dip"
            style="@style/common_style_for_24sp"
            android:text="@string/time_set1"/>
         <TextView
             android:id="@+id/text_time_set"
             android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:background="@drawable/more_bg"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_margin="10dip"
        android:background="@drawable/layout_bg" >

        <TextView
            android:id="@+id/text_timezone"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:drawableLeft="@drawable/shiqu" 
            style="@style/common_style_for_20sp_white"
            android:text="@string/timezone"/>
        <TextView
            android:id="@+id/text_timezone_content"
            android:layout_toRightOf="@+id/text_timezone"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:layout_marginLeft="20dip"
            style="@style/common_style_for_24sp"
            android:text="@string/timezone_set1"/>
         <TextView
             android:id="@+id/text_timezone_set"
             android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="50dip"
            android:gravity="center"
            android:background="@drawable/more_bg"
            />
    </RelativeLayout>
</LinearLayout>